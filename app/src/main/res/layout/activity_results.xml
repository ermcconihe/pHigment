<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/rrnhea0yqo7i"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:layout_marginHorizontal="4dp"
        android:clipChildren="false"
        android:clipToPadding="false"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ry6mj056jf6b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/s74777580sw8cr18bf4fbf8"
                android:paddingBottom="852dp"
                android:paddingHorizontal="16dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/re5jlum6ah4"
                    android:textColor="#171D1B"
                    android:textSize="14sp"
                    android:layout_marginVertical="10dp"
                    android:layout_marginEnd="152dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="9:30"
                    />
                <LinearLayout
                    android:id="@+id/rv0xvmjrvg9"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/cr100b2e2e2e"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/r6bttriye49f"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <ImageView
                    android:id="@+id/r2ehtb5xzelk"
                    android:layout_width="46dp"
                    android:layout_height="40dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:clipChildren="false"
            android:clipToPadding="false"
            android:id="@+id/rl7588wdkhd_rl"
            android:layout_marginBottom="-1dp"
            android:layout_marginEnd="-4dp"
            android:layout_marginStart="-4dp"
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/rl7588wdkhd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitXY"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/rmq84fsfn31"
                    android:layout_width="match_parent"
                    android:layout_height="654dp"
                    android:layout_marginHorizontal="21dp"
                    android:layout_marginTop="36dp"
                    android:layout_marginBottom="19dp"
                    android:background="@drawable/cr28be9efec"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical"
                    android:paddingTop="24dp"
                    android:paddingBottom="43dp">

                    <TextView
                        android:id="@+id/rgnos8djx38"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="48dp"
                        android:layout_marginBottom="45dp"
                        android:text="Results"
                        android:textColor="#1D1B1B"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/rw9d2mxiww3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="29dp"
                        android:layout_marginBottom="0dp"
                        android:text="My Health"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/RecentTests"
                        android:layout_width="wrap_content"
                        android:layout_height="44dp"
                        android:layout_marginStart="29dp"
                        android:layout_marginBottom="18dp"
                        android:text="Recent Tests"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/ra07via2hk7l"
                        android:layout_width="match_parent"
                        android:layout_height="211dp"
                        android:layout_marginHorizontal="15dp"
                        android:layout_marginBottom="0dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/demo_graph" />

                    <TextView
                        android:id="@+id/rqkdqzszvbm"
                        android:layout_width="226dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="29dp"
                        android:text="\n\nTest Result: \n pH value:  \n\nAnalysis:\n \n\n\n\n\n"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/pHValue"
                        android:layout_width="300dp"
                        android:layout_height="80dp"
                        android:layout_marginTop="-110dp"
                        android:layout_marginBottom="30dp"
                        android:lineSpacingExtra="4dp"
                        android:minLines="10"
                        android:text="pH ..."
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <TextView
                        android:id="@+id/pHAnalysisValue"
                        android:layout_width="180dp"
                        android:layout_height="80dp"
                        android:layout_marginTop="-40dp"
                        android:layout_marginBottom="30dp"
                        android:lineSpacingExtra="4dp"
                        android:minLines="10"
                        android:text="Analysis ..."
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rk9eukme4qmd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/sbec9c5sw1cr28be9efec"
                    android:paddingVertical="47dp"
                    android:layout_marginBottom="75dp"
                    android:layout_marginHorizontal="21dp"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/rnt7zl2vsyz"
                        android:textColor="#1D1B1B"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:layout_width="235dp"
                        android:layout_height="100dp"
                        android:textAlignment="center"

                        android:text="Take another photo"
                        />
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
